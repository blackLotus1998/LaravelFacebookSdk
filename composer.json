{
  "name": "blacklotus1998/laravel-facebook-sdk",
  "description": "Facebook SDK integration for Laravel",
  "keywords": [
    "facebook-sdk",
    "laravel"
  ],
  "homepage": "https://github.com/blackLotus1998/LaravelFacebookSdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Yee Jia Wei",
      "email": "blacklotus1998@outlook.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/auth": "^6.0|^7.0|^8.0",
    "illuminate/config": "^6.0|^7.0|^8.0",
    "illuminate/database": "^6.0|^7.0|^8.0",
    "illuminate/routing": "^6.0|^7.0|^8.0",
    "illuminate/session": "^6.0|^7.0|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "facebook/graph-sdk": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Blacklotus\\LaravelFacebookSdk\\": "src/LaravelFacebookSdk",
      "Blacklotus\\LaravelFacebookSdk\\Test": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Blacklotus\\LaravelFacebookSdk\\LaravelFacebookSdkServiceProvider"
      ],
      "aliases": {
        "Facebook": "Blacklotus\\LaravelFacebookSdk\\FacebookFacade"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
